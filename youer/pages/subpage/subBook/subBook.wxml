<view class="weui-panel weui-panel_access">
  <view class="weui-panel__hd">{{title}}</view>
  <view class="weui-panel__bd">
    <navigator wx:for="{{book_list.content}}" url="{{item.url}}" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
      <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
        <image class="weui-media-box__thumb" src="{{item.pic}}" />
      </view>
      <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
        <view class="weui-media-box__title">{{item.name}}</view>
        <view class="weui-media-box__desc">{{item.description}}</view>
      </view>
    </navigator>
  </view>
</view>